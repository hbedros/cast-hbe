# CAST Highlight Configuration
# Remove .example extension from the file and update below with your credentials

cast:
  base_url: "https://rpa.casthighlight.com/WS2"
  company_id: "YOUR_COMPANY_ID"
  login: "YOUR_LOGIN"
  password: "YOUR_PASSWORD"
  docker_image: "casthighlight/hl-cli"

github:
  token: "YOUR_GITHUB_TOKEN"

amazon_q:
  email: "YOUR_AMAZON_Q_EMAIL"
  portfolio: "AmazonQ"
  region: "us-east-1"

output:
  base_dir: "/tmp/output" 